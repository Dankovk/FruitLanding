{
    "path": {
        "font": {
            "src": "src/fonts",
            "dest": "dist/fonts"
        },
        "html": {
            "src": "src",
            "dest": "dist"
        },
        "image": {
            "src": "src/images",
            "dest": "dist/images"
        },
        "script": {
            "src": "src/scripts",
            "dest": "dist/scripts",
            "vendor": "src/scripts/vendor"
        },
        "sprite": {
            "src": "src/sprites",
            "dest": "dist/images"
        },
        "style": {
            "src": "src/styles",
            "dest": "dist/styles",
            "vendor": "src/styles/vendor"
        },
        "http": "/dist/"
    }
}
